HOST=0.0.0.0
PORT=8002
WORKER_COUNT=1
ENVIRONMENT=development

CACHE_BACKEND=local
RATE_LIMIT_BACKEND=local

REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=yourStrongPassword

GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD=supersecurepassword

# Completely free setup
USE_LOCAL_MODEL=true
LOCAL_MODEL_URL=http://127.0.0.1:1234
SEARCH_PROVIDER=duckduckgo
DUCKDUCKGO_MAX_RESULTS=10

# Paid setup (OpenAI + Tavily) - LEGACY
USE_LOCAL_MODEL=false
OPENAI_API_KEY=[OPENAI_API_KEY]
SEARCH_PROVIDER=tavily
TAVILY_API_KEY=tvly-your-key

# Google Gemini Configuration (Primary AI Provider)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_CHAT_MODEL=gemini-2.0-flash-exp
GEMINI_EMBEDDING_MODEL=text-embedding-004

# Qdrant Vector Database
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
QDRANT_COLLECTION_NAME=voyage_policies
EMBEDDING_DIMENSION=768

# PostgreSQL Database (for audit logs and policy metadata)
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/voyage_policies

LANGFUSE_HOST=[LANGFUSE_HOST]
LANGFUSE_PUBLIC_KEY=[LANGFUSE_PUBLIC_KEY]
LANGFUSE_SECRET_KEY=[LANGFUSE_SECRET_KEY]
